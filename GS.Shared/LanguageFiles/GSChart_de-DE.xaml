<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Dialog -->
    <system:String x:Key="diaAccept">Akzeptieren</system:String>
    <system:String x:Key="diaCancel">Abbrechen</system:String>
    <system:String x:Key="diaOk">Ok</system:String>
    <system:String x:Key="diaStart">Start</system:String>
    <system:String x:Key="diaStop">Stop</system:String>

    <!--Converters -->
    <system:String x:Key="cvtInvNumber">Ungültige Nummer</system:String>
    <system:String x:Key="cvtColorToBrush">ColorToBrushConverter unterstützt nur die Konvertierung von Farbe und Zeichenfolge</system:String>
    <system:String x:Key="cvtEnumErr1">Typ muss für einen Aufzählungstyp sein</system:String>
    <system:String x:Key="cvtEnumErr2">Der Aufzählungstyp (EnumType) muss angegeben werden</system:String>
    <system:String x:Key="cvtInvIP">Ungültige IP</system:String>
    <system:String x:Key="cvtRequired">Erforderlich</system:String>

    <!--ChartViewer -->
    <system:String x:Key="chtNumber">#</system:String>
    <system:String x:Key="chtStart">Start</system:String>
    <system:String x:Key="chtEnd">Ende</system:String>
    <system:String x:Key="chtLength">Länge</system:String>
    <system:String x:Key="chtType">Typ</system:String>
    <system:String x:Key="chtOpen">Datei öffnen</system:String>
    <system:String x:Key="chtQuality">Qualität</system:String>
    <system:String x:Key="chtViewLog">Log anzeigen</system:String>
    <system:String x:Key="chtZoom">Vergrößern:</system:String>
    <system:String x:Key="chtNoValidRecords">Keine gültigen Einträge gefunden</system:String>

    <system:String x:Key="optLanguage">Sprachen</system:String>
</ResourceDictionary>
 
